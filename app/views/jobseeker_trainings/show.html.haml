- model_class = JobseekerTraining
.page-header
  %h1=t '.title', :default => model_class.model_name.human

%p
  %strong= model_class.human_attribute_name(:user_id) + ':'
  %br
  = @jobseeker_training.user_id
%p
  %strong= model_class.human_attribute_name(:title) + ':'
  %br
  = @jobseeker_training.title
%p
  %strong= model_class.human_attribute_name(:status) + ':'
  %br
  = @jobseeker_training.status
%p
  %strong= model_class.human_attribute_name(:transfer) + ':'
  %br
  = @jobseeker_training.transfer
%p
  %strong= model_class.human_attribute_name(:technology) + ':'
  %br
  = @jobseeker_training.technology
%p
  %strong= model_class.human_attribute_name(:instruction_mod) + ':'
  %br
  = @jobseeker_training.instruction_mod
%p
  %strong= model_class.human_attribute_name(:accomodation) + ':'
  %br
  = @jobseeker_training.accomodation
%p
  %strong= model_class.human_attribute_name(:description) + ':'
  %br
  = @jobseeker_training.description

.form-actions
  = link_to t('.back', :default => t("helpers.links.back")), jobseeker_trainings_path, :class => 'btn'
  = link_to t('.edit', :default => t("helpers.links.edit")), edit_jobseeker_training_path(@jobseeker_training), :class => 'btn'
  = link_to t('.destroy', :default => t("helpers.links.destroy")), jobseeker_training_path(@jobseeker_training), :method => "delete", :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) }, :class => 'btn btn-danger'
